<html lang="en">

  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Dealership Pro</title>
    <link rel="stylesheet" href="/css/jass.css" />
    <link rel="stylesheet" href="../css/tailwind.css" />
    <link rel="stylesheet" href="../css/custom.css" />
    <link
      href="https://unpkg.com/tailwindcss@^2/dist/tailwind.min.css"
      rel="stylesheet"
    />
    
  </head>
   

  <body>
    <!-- This example requires Tailwind CSS v2.0+ -->
    <div
      class="dashboard-nav"
      class="flex flex-col flex-grow border-r border-gray-200 pt-5 pb-4"
    >
      <div class="flex items-center flex-shrink-0 px-4 space-y-5">
        {{! <img
          class="h-8 w-auto"
          src="https://tailwindui.com/img/logos/workflow-logo-indigo-600-mark-gray-800-text.svg"
          alt="Workflow"
        /> }}
        <h1 class="text-2xl text-white">Dealership Pro</h1>
      </div>
      <div class="mt-5 flex-grow flex flex-col">
        <nav class="flex-1 space-y-1" aria-label="Sidebar">
          <!-- Current: "bg-indigo-50 border-indigo-600 text-indigo-600", Default: "border-transparent text-gray-600 hover:bg-gray-50 hover:text-gray-900" -->
          <a
            href="/dealer"
        {{#if home}}
          class="bg-white border-black-600 text-black-600 group flex items-center px-3 py-2 text-sm font-medium border-l-4"
        {{else}}
          class="border-transparent text-white hover:bg-gray-50 hover:text-gray-900 group flex items-center px-3 py-2 text-sm font-medium border-l-4"
        {{/if}}
          >
            <!--
          Heroicon name: outline/home

          Current: "text-indigo-500", Default: "text-gray-400 group-hover:text-gray-500"
        -->
            <svg
          {{#if home}}
            class="text-black group-hover:text-gray-500 mr-3 h-6 w-6"
            {{else}}
              class="text-white group-hover:text-gray-500 mr-3 h-6 w-6"
            {{/if}}
              xmlns="http://www.w3.org/2000/svg"
              fill="none"
              viewBox="0 0 24 24"
              stroke="currentColor"
              aria-hidden="true"
            >
              <path
                stroke-linecap="round"
                stroke-linejoin="round"
                stroke-width="2"
                d="M3 12l2-2m0 0l7-7 7 7M5 10v10a1 1 0 001 1h3m10-11l2 2m-2-2v10a1 1 0 01-1 1h-3m-6 0a1 1 0 001-1v-4a1 1 0 011-1h2a1 1 0 011 1v4a1 1 0 001 1m-6 0h6"
              ></path>
            </svg>
            Home
          </a>

          <a
            href="/dealer/inventory"
      {{#if inventory}}
          class="bg-white border-black-600 text-black-600 group flex items-center px-3 py-2 text-sm font-medium border-l-4"
        {{else}}
          class="border-transparent text-white hover:bg-gray-50 hover:text-gray-900 group flex items-center px-3 py-2 text-sm font-medium border-l-4"
        {{/if}}
          >
            <!-- Heroicon name: outline/users -->
            <svg
           {{#if inventory}}
            class="text-black group-hover:text-gray-500 mr-3 h-6 w-6"
            {{else}}
              class="text-white group-hover:text-gray-500 mr-3 h-6 w-6"
            {{/if}}
              xmlns="http://www.w3.org/2000/svg"
              fill="none"
              viewBox="0 0 24 24"
              stroke="currentColor"
              aria-hidden="true"
            >
              <path
                stroke-linecap="round"
                stroke-linejoin="round"
                stroke-width="2"
                d="M12 4.354a4 4 0 110 5.292M15 21H3v-1a6 6 0 0112 0v1zm0 0h6v-1a6 6 0 00-9-5.197M13 7a4 4 0 11-8 0 4 4 0 018 0z"
              ></path>
            </svg>
            Inventory
          </a>

          <a
            href="/dealer/sold"
        {{#if soldcars}}
          class="bg-white border-black-600 text-black-600 group flex items-center px-3 py-2 text-sm font-medium border-l-4"
        {{else}}
          class="border-transparent text-white hover:bg-gray-50 hover:text-gray-900 group flex items-center px-3 py-2 text-sm font-medium border-l-4"
        {{/if}}
          >
            <!-- Heroicon name: outline/folder -->
            <svg
              class="text-white group-hover:text-gray-500 mr-3 h-6 w-6"
              xmlns="http://www.w3.org/2000/svg"
              fill="none"
              viewBox="0 0 24 24"
              stroke="currentColor"
              aria-hidden="true"
            >
              <path
                stroke-linecap="round"
                stroke-linejoin="round"
                stroke-width="2"
                d="M3 7v10a2 2 0 002 2h14a2 2 0 002-2V9a2 2 0 00-2-2h-6l-2-2H5a2 2 0 00-2 2z"
              ></path>
            </svg>
            Sold Cars
          </a>

          <a
            href="#"
        {{#if contacts}}
          class="bg-white border-black-600 text-black-600 group flex items-center px-3 py-2 text-sm font-medium border-l-4"
        {{else}}
          class="border-transparent text-white hover:bg-gray-50 hover:text-gray-900 group flex items-center px-3 py-2 text-sm font-medium border-l-4"
        {{/if}}
          >
            <!-- Heroicon name: outline/calendar -->
            <svg
            {{#if contacts}}
            class="text-black group-hover:text-gray-500 mr-3 h-6 w-6"
            {{else}}
              class="text-white group-hover:text-gray-500 mr-3 h-6 w-6"
            {{/if}}
              xmlns="http://www.w3.org/2000/svg"
              fill="none"
              viewBox="0 0 24 24"
              stroke="currentColor"
              aria-hidden="true"
            >
              <path
                stroke-linecap="round"
                stroke-linejoin="round"
                stroke-width="2"
                d="M8 7V3m8 4V3m-9 8h10M5 21h14a2 2 0 002-2V7a2 2 0 00-2-2H5a2 2 0 00-2 2v12a2 2 0 002 2z"
              ></path>
            </svg>
            Contacts
          </a>

          <a
            href="#"
       {{#if analytics}}
          class="bg-white border-black-600 text-black-600 group flex items-center px-3 py-2 text-sm font-medium border-l-4"
        {{else}}
          class="border-transparent text-white hover:bg-gray-50 hover:text-gray-900 group flex items-center px-3 py-2 text-sm font-medium border-l-4"
        {{/if}}
          >
            <!-- Heroicon name: outline/inbox -->
            <svg
            {{#if analytics}}
            class="text-black group-hover:text-gray-500 mr-3 h-6 w-6"
            {{else}}
              class="text-white group-hover:text-gray-500 mr-3 h-6 w-6"
            {{/if}}
              xmlns="http://www.w3.org/2000/svg"
              fill="none"
              viewBox="0 0 24 24"
              stroke="currentColor"
              aria-hidden="true"
            >
              <path
                stroke-linecap="round"
                stroke-linejoin="round"
                stroke-width="2"
                d="M20 13V6a2 2 0 00-2-2H6a2 2 0 00-2 2v7m16 0v5a2 2 0 01-2 2H6a2 2 0 01-2-2v-5m16 0h-2.586a1 1 0 00-.707.293l-2.414 2.414a1 1 0 01-.707.293h-3.172a1 1 0 01-.707-.293l-2.414-2.414A1 1 0 006.586 13H4"
              ></path>
            </svg>
            Analytics
          </a>

          <a
            href="/dealer/newcar"
            
        {{#if newcar}}
          class="bg-white border-black-600 text-black-600 group flex items-center px-3 py-2 text-sm font-medium border-l-4"
        {{else}}
          class="border-transparent text-white hover:bg-gray-50 hover:text-gray-900 group flex items-center px-3 py-2 text-sm font-medium border-l-4"
        {{/if}}
            
          >
            <!-- Heroicon name: outline/chart-bar -->
            <svg
            {{#if newcar}}
            class="text-black group-hover:text-gray-500 mr-3 h-6 w-6"
            {{else}}
              class="text-white group-hover:text-gray-500 mr-3 h-6 w-6"
            {{/if}}
              xmlns="http://www.w3.org/2000/svg"
              fill="none"
              viewBox="0 0 24 24"
              stroke="currentColor"
              aria-hidden="true"
            >
              <path
                stroke-linecap="round"
                stroke-linejoin="round"
                stroke-width="2"
                d="M9 19v-6a2 2 0 00-2-2H5a2 2 0 00-2 2v6a2 2 0 002 2h2a2 2 0 002-2zm0 0V9a2 2 0 012-2h2a2 2 0 012 2v10m-6 0a2 2 0 002 2h2a2 2 0 002-2m0 0V5a2 2 0 012-2h2a2 2 0 012 2v14a2 2 0 01-2 2h-2a2 2 0 01-2-2z"
              ></path>
            </svg>
            Add a car
          </a>
        </nav>
      </div>
  {{#if loggedIn}}
      <div class="flex justify-center align-bottom logButton">
        <button id="logout" type="button" class="ml-3 px-4 py-2 border border-transparent rounded-md shadow-sm text-sm font-medium text-black bg-white hover:bg-gray-50 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-gray-200">
          LOGOUT
        </button>
      </div>
  {{else}}
     <div class="flex justify-center align-bottom logButton">
        <a href="/login">        
          <button id="login" type="button" class="ml-3 inline-flex items-center px-4 py-2 border border-transparent rounded-md shadow-sm text-sm font-medium text-black bg-white hover:bg-gray-50 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-gray-200">
            LOGIN
          </button>
        </a>
    </div>
  {{/if}}
    </div>
    <div class="dashboard-card">
      {{{body}}}
    </div>
{{#if loggedIn}}
  <script src="/js/logout.js"></script>
  {{/if}}
  </body>

</html>