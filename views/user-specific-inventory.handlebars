<section class="car-container">
    <button onclick='window.location.href="http://localhost:3001/user/inventory"'>Back</button>
    <article class="car-title">
        <h1>{{car.car_year}} {{car.make}} {{car.model}} {{car.trim}}</h1>
    </article>
    <article class="car-holder">
        <article class="car-image-holder">              
            <img class="car-image" src={{car.image}} alt="Car image">
        </article>
        <article class="car-info-holder">
            <h2>Mileage: {{car.mileage}}</h2>
            <h2>Color: {{car.color}}</h2>
            <h2>Interior: {{car.interior_color}}</h2>
        </article>
        <article class="car-price-holder">
            <h2>Price: {{car.price}}</h2>
        </article>
    </article>
</section>