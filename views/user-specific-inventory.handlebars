<section class="car-container">
    <article class="car-title">
        
    </article>
    <article class="car-holder">
        <article class="car-image-holder">
            <img class="car-image" src={{car.image}} alt="Car image">
        </article>
        <article class="car-info-holder">
            <h2>{{car.car_year}} {{car.model}} {{car.trim}}</h2>
            <h3>{{car.make}}</h3>
            <article class="car-price-holder">
                <h2>${{car.price}}</h2>
                <h2>|</h2>
                <h2>{{car.mileage}} miles</h2>
            </article>
            <h1>Vin# {{car.vin}}</h1>
        </article>
    </article>
</section>